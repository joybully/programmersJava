    <!-- to1, to2 선언 -->
    <bean id="to1" class="com.example.di05.BoardTO">
        <property name="seq" value="1"/>
        <property name="subject" value="제목1"/>
    </bean>
    <bean id="to2" class="com.example.di05.BoardTO">
        <property name="seq" value="2"/>
        <property name="subject" value="제목2"/>
    </bean>
    <bean id="boardMapTO" class="com.example.di05.BoardMapTO">
        <property name="userMaps">
            <map>
                <entry key="1">
                    <value>제목1</value>
                </entry>
                <entry key="2">
                    <value>제목2</value>
                </entry>
            </map>
        </property>
        <property name="boardMaps">
            <map>
                <entry key="1">
                    <ref bean="to1"/>
                </entry>
                <entry key="2">
                    <ref bean="to2"/>
                </entry>
            </map>
        </property>
    </bean>
</beans>