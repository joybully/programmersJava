    <bean id="to1" class="com.example.di04.BoardTO">
        <property name="seq" value="1"/>
        <property name="subject" value="제목 1"/>
    </bean>
    <bean id="to2" class="com.example.di04.BoardTO">
        <property name="seq" value="2"/>
        <property name="subject" value="제목 2"/>
    </bean>
    <bean id="listTO" class="com.example.di04.BoardListTO">
        <property name="userLists">
            <list>
                <value>홍길동</value>
                <value>박문수</value>
            </list>
        </property>
        <property name="boardLists">
            <list>
                <ref bean="to1"/>
                <ref bean="to2"/>
            </list>
        </property>
    </bean>
</beans>